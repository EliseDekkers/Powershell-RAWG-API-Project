$graphData = $response.results | Where-Object { $_.playtime -ne $null -and $_.rating -ne $null } | ForEach-Object {
    [PSCustomObject]@{
        X = [int][math]::Round($_.released)  # Released waarde afronden naar een int
        Y = [int][math]::Round($_.rating * 10)  # Rating maal 10 en afronden naar een int
    }
}

$datapoints = $graphData | ForEach-Object {
    @($_.X, $_.Y)  # Maak een array van de X en Y waarden
}

Show-Graph -Datapoints $datapoints -GraphTitle "Released vs Rating" -XAxisTitle "Released" -YAxisTitle "Rating" -Type "Line"
